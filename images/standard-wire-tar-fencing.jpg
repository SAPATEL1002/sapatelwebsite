<svg xmlns="http://www.w3.org/2000/svg" width="1000" height="600" viewBox="0 0 1000 600">
  <rect width="1000" height="600" fill="#f5f5f5"/>
  <g transform="translate(500, 300)">
    <!-- Fence posts -->
    <rect x="-400" y="-100" width="30" height="300" fill="#8B4513"/>
    <rect x="-200" y="-100" width="30" height="300" fill="#8B4513"/>
    <rect x="0" y="-100" width="30" height="300" fill="#8B4513"/>
    <rect x="200" y="-100" width="30" height="300" fill="#8B4513"/>
    <rect x="400" y="-100" width="30" height="300" fill="#8B4513"/>
    
    <!-- Wire mesh -->
    <g stroke="#555" stroke-width="3" fill="none">
      <!-- Horizontal lines -->
      <line x1="-450" y1="-80" x2="450" y2="-80"/>
      <line x1="-450" y1="-40" x2="450" y2="-40"/>
      <line x1="-450" y1="0" x2="450" y2="0"/>
      <line x1="-450" y1="40" x2="450" y2="40"/>
      <line x1="-450" y1="80" x2="450" y2="80"/>
      <line x1="-450" y1="120" x2="450" y2="120"/>
      <line x1="-450" y1="160" x2="450" y2="160"/>
      
      <!-- Vertical lines -->
      <line x1="-450" y1="-100" x2="-450" y2="200"/>
      <line x1="-350" y1="-100" x2="-350" y2="200"/>
      <line x1="-250" y1="-100" x2="-250" y2="200"/>
      <line x1="-150" y1="-100" x2="-150" y2="200"/>
      <line x1="-50" y1="-100" x2="-50" y2="200"/>
      <line x1="50" y1="-100" x2="50" y2="200"/>
      <line x1="150" y1="-100" x2="150" y2="200"/>
      <line x1="250" y1="-100" x2="250" y2="200"/>
      <line x1="350" y1="-100" x2="350" y2="200"/>
      <line x1="450" y1="-100" x2="450" y2="200"/>
    </g>
    
    <!-- Tar coating representation -->
    <rect x="-450" y="-100" width="900" height="300" fill="rgba(0,0,0,0.2)" rx="5" ry="5"/>
    
    <!-- Ground -->
    <rect x="-500" y="200" width="1000" height="100" fill="#8a6d3b"/>
    <rect x="-500" y="200" width="1000" height="20" fill="#a1887f"/>
  </g>
  <text x="500" y="50" font-family="Arial" font-size="40" text-anchor="middle" fill="#333">Standard Wire Tar Fencing</text>
</svg>