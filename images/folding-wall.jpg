<svg xmlns="http://www.w3.org/2000/svg" width="1000" height="600" viewBox="0 0 1000 600">
  <rect width="1000" height="600" fill="#f5f5f5"/>
  <g transform="translate(500, 300)">
    <!-- Folding wall sections -->
    <g transform="rotate(-15) translate(-300, 0)">
      <rect x="-50" y="-150" width="100" height="300" fill="#a1887f" stroke="#555" stroke-width="3"/>
      <line x1="-50" y1="-100" x2="50" y2="-100" stroke="#555" stroke-width="2"/>
      <line x1="-50" y1="-50" x2="50" y2="-50" stroke="#555" stroke-width="2"/>
      <line x1="-50" y1="0" x2="50" y2="0" stroke="#555" stroke-width="2"/>
      <line x1="-50" y1="50" x2="50" y2="50" stroke="#555" stroke-width="2"/>
      <line x1="-50" y1="100" x2="50" y2="100" stroke="#555" stroke-width="2"/>
    </g>
    
    <g transform="translate(-100, 0)">
      <rect x="-50" y="-150" width="100" height="300" fill="#a1887f" stroke="#555" stroke-width="3"/>
      <line x1="-50" y1="-100" x2="50" y2="-100" stroke="#555" stroke-width="2"/>
      <line x1="-50" y1="-50" x2="50" y2="-50" stroke="#555" stroke-width="2"/>
      <line x1="-50" y1="0" x2="50" y2="0" stroke="#555" stroke-width="2"/>
      <line x1="-50" y1="50" x2="50" y2="50" stroke="#555" stroke-width="2"/>
      <line x1="-50" y1="100" x2="50" y2="100" stroke="#555" stroke-width="2"/>
    </g>
    
    <g transform="rotate(15) translate(100, 0)">
      <rect x="-50" y="-150" width="100" height="300" fill="#a1887f" stroke="#555" stroke-width="3"/>
      <line x1="-50" y1="-100" x2="50" y2="-100" stroke="#555" stroke-width="2"/>
      <line x1="-50" y1="-50" x2="50" y2="-50" stroke="#555" stroke-width="2"/>
      <line x1="-50" y1="0" x2="50" y2="0" stroke="#555" stroke-width="2"/>
      <line x1="-50" y1="50" x2="50" y2="50" stroke="#555" stroke-width="2"/>
      <line x1="-50" y1="100" x2="50" y2="100" stroke="#555" stroke-width="2"/>
    </g>
    
    <g transform="rotate(30) translate(300, 0)">
      <rect x="-50" y="-150" width="100" height="300" fill="#a1887f" stroke="#555" stroke-width="3"/>
      <line x1="-50" y1="-100" x2="50" y2="-100" stroke="#555" stroke-width="2"/>
      <line x1="-50" y1="-50" x2="50" y2="-50" stroke="#555" stroke-width="2"/>
      <line x1="-50" y1="0" x2="50" y2="0" stroke="#555" stroke-width="2"/>
      <line x1="-50" y1="50" x2="50" y2="50" stroke="#555" stroke-width="2"/>
      <line x1="-50" y1="100" x2="50" y2="100" stroke="#555" stroke-width="2"/>
    </g>
    
    <!-- Ground -->
    <rect x="-500" y="150" width="1000" height="150" fill="#8a6d3b"/>
    <rect x="-500" y="150" width="1000" height="20" fill="#a1887f"/>
  </g>
  <text x="500" y="50" font-family="Arial" font-size="40" text-anchor="middle" fill="#333">Folding Wall</text>
</svg>