<svg xmlns="http://www.w3.org/2000/svg" width="1000" height="600" viewBox="0 0 1000 600">
  <rect width="1000" height="600" fill="#f5f5f5"/>
  <g transform="translate(500, 300)">
    <!-- Stone wall base -->
    <g>
      <!-- Row 1 -->
      <rect x="-450" y="0" width="140" height="65" fill="#78909c" stroke="#555" stroke-width="2" rx="5" ry="5"/>
      <rect x="-300" y="0" width="110" height="65" fill="#607d8b" stroke="#555" stroke-width="2" rx="5" ry="5"/>
      <rect x="-180" y="0" width="130" height="65" fill="#78909c" stroke="#555" stroke-width="2" rx="5" ry="5"/>
      <rect x="-40" y="0" width="100" height="65" fill="#607d8b" stroke="#555" stroke-width="2" rx="5" ry="5"/>
      <rect x="70" y="0" width="150" height="65" fill="#78909c" stroke="#555" stroke-width="2" rx="5" ry="5"/>
      <rect x="230" y="0" width="120" height="65" fill="#607d8b" stroke="#555" stroke-width="2" rx="5" ry="5"/>
      <rect x="360" y="0" width="90" height="65" fill="#78909c" stroke="#555" stroke-width="2" rx="5" ry="5"/>
      
      <!-- Row 2 -->
      <rect x="-450" y="75" width="100" height="75" fill="#607d8b" stroke="#555" stroke-width="2" rx="5" ry="5"/>
      <rect x="-340" y="75" width="150" height="75" fill="#78909c" stroke="#555" stroke-width="2" rx="5" ry="5"/>
      <rect x="-180" y="75" width="120" height="75" fill="#607d8b" stroke="#555" stroke-width="2" rx="5" ry="5"/>
      <rect x="-50" y="75" width="140" height="75" fill="#78909c" stroke="#555" stroke-width="2" rx="5" ry="5"/>
      <rect x="100" y="75" width="110" height="75" fill="#607d8b" stroke="#555" stroke-width="2" rx="5" ry="5"/>
      <rect x="220" y="75" width="130" height="75" fill="#78909c" stroke="#555" stroke-width="2" rx="5" ry="5"/>
      <rect x="360" y="75" width="90" height="75" fill="#607d8b" stroke="#555" stroke-width="2" rx="5" ry="5"/>
    </g>
    
    <!-- Wire fence on top -->
    <g transform="translate(0, -100)">
      <!-- Fence posts -->
      <rect x="-400" y="-50" width="20" height="150" fill="#8B4513"/>
      <rect x="-200" y="-50" width="20" height="150" fill="#8B4513"/>
      <rect x="0" y="-50" width="20" height="150" fill="#8B4513"/>
      <rect x="200" y="-50" width="20" height="150" fill="#8B4513"/>
      <rect x="400" y="-50" width="20" height="150" fill="#8B4513"/>
      
      <!-- Wire mesh -->
      <g stroke="#555" stroke-width="2" fill="none">
        <!-- Horizontal lines -->
        <line x1="-450" y1="-30" x2="450" y2="-30"/>
        <line x1="-450" y1="0" x2="450" y2="0"/>
        <line x1="-450" y1="30" x2="450" y2="30"/>
        <line x1="-450" y1="60" x2="450" y2="60"/>
        
        <!-- Vertical lines -->
        <line x1="-350" y1="-50" x2="-350" y2="100"/>
        <line x1="-300" y1="-50" x2="-300" y2="100"/>
        <line x1="-250" y1="-50" x2="-250" y2="100"/>
        <line x1="-150" y1="-50" x2="-150" y2="100"/>
        <line x1="-100" y1="-50" x2="-100" y2="100"/>
        <line x1="-50" y1="-50" x2="-50" y2="100"/>
        <line x1="50" y1="-50" x2="50" y2="100"/>
        <line x1="100" y1="-50" x2="100" y2="100"/>
        <line x1="150" y1="-50" x2="150" y2="100"/>
        <line x1="250" y1="-50" x2="250" y2="100"/>
        <line x1="300" y1="-50" x2="300" y2="100"/>
        <line x1="350" y1="-50" x2="350" y2="100"/>
      </g>
    </g>
    
    <!-- Ground -->
    <rect x="-500" y="150" width="1000" height="150" fill="#8a6d3b"/>
    <rect x="-500" y="150" width="1000" height="20" fill="#a1887f"/>
  </g>
  <text x="500" y="50" font-family="Arial" font-size="40" text-anchor="middle" fill="#333">Wire Fencing with Stone Wall</text>
</svg>